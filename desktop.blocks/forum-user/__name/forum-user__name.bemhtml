block('forum-user').elem('name').def()(function() {
    var mix = this.ctx.mix || [];

    return applyCtx({
        block: 'link',
        url: this.ctx.url,
        title: this.ctx.title,
        target: this.ctx.target || undefined,
        content: this.ctx.content,
        mix: (function() {
            mix.push({ block: 'forum-user', elem: 'name' });

            return mix;
        })()
    });
});
