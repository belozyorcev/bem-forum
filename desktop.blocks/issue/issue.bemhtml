block('issue')(
    js()(true),
    match(function() { return this.ctx.data; }).js()(function() {
        var issue = this.ctx.data,
            labels = issue.labels,
            labelsName = [];

        labels.forEach(function(label) {
            labelsName.push(label.name);
        });

        return {
            number: issue.number,
            labels: labelsName
        };
    })
);
