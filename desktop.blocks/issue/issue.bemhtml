block('issue')(
    js()(true),
    match(function() { return this.ctx.data || this.ctx.issue; }).js()(function() {
        var issue = this.ctx.data || this.ctx.issue,
            labels = issue.labels,
            labelsName = [];

        labels.forEach(function(label) {
            labelsName.push(label.name);
        });

        return {
            number: issue.number,
            labels: labelsName,
            forumUrl: this.ctx.forumUrl
        };
    })
);
