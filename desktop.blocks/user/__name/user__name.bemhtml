block('user').elem('name').def()(function() {
    var mix = this.ctx.mix || [];

    return applyCtx({
        block: 'link',
        url: 'github.com/' + this.ctx.url,
        title: this.ctx.title,
        content: this.ctx.content,
        mix: (function() {
            mix.push({ block: 'user', elem: 'name' });

            return mix;
        })()
    });
});
