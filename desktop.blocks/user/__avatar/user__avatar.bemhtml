block('user').elem('avatar').def()(function() {
    var mix = this.ctx.mix || [];

    return applyCtx({
        block: 'image',
        url: this.ctx.url,
        alt: this.ctx.alt,
        mix: (function() {
            mix.push({ block: 'user', elem: 'avatar' });

            return mix;
        })()
    });
});
